					},
					{
						"key": "resolve_sibling_scope",
						"type": "object"
					},
					{
						"key": "source_control_name",
						"type": "object"
					},
					{
						"key": "source_property_name",
						"type": "object"
					},
					{
						"key": "target_property_name",
						"type": "object"
					}
				]
			}
		]
