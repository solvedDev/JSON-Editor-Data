						"type": "object"
					},
					{
						"key": "tiers",
						"type": "object"
					},
					{
						"key": "entries",
						"type": "list"
					},
					{
						"key": "conditions",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is(tiers)",
				"method": "replace",
				"propose": [
