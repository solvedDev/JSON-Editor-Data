			},
			{
				"recognize": "$is(count)",
				"method": "replace",
				"propose": [
					{
						"key": "1",
						"type": "number"
					}
				] 
			}
		]
	},
	"ui": {
		"define": "$has_child(type) or $is(namespace) or $contains(@)",
		"basic": [ "vanilla.ui" ],
		"extend": [
