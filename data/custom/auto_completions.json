						"type": "string"
					},
					{
						"key": "large",
						"type": "string"
					}
				]
			},
			//Panel/Stack panel
			{
				"recognize": "$is(panel)_on_$child_value(type) or $is(stack_panel)_on_$child_value(type)",
				"method": "replace",
				"propose": [
					{
						"key": "size",
						"type": "object"
					}
				] 
			},
			//General: objects
			{
				"recognize": "$is_path(.) and $is_root_not or $is_path(../controls/./.)",
				"method": "append",
				"propose": [
					{
						"key": "anchor_to",
						"type": "object"
					},
					{
						"key": "anchor_from",
						"type": "object"
					},
					{
						"key": "controls",
						"type": "list"
					},
					{
						"key": "bindings",
						"type": "list"
					},
					{
						"key": "button_mappings",
						"type": "list"
					}
				] 
			},

			//Bindings
			{
				"recognize": "$is_path(../bindings/.)",
				"method": "replace",
				"propose": [
					{
						"key": "binding_name",
						"type": "object"
					},
					{
						"key": "binding_name_override",
						"type": "object"
					},
					{
						"key": "binding_type",
						"type": "object"
					},
					{
						"key": "binding_collection_name",
