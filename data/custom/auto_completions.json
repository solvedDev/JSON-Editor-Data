{
	"entity": {
		"define": "$is_path(minecraft:entity/..)", //Defines what an entity file is
		"basic": [ "vanilla.entities", "documentation.components" ], //Links to var
		"extend": [
			//GENERAL
			{
				"recognize": "($is(id) or $contains(entity_type) or $is_path(../transformation/into))", //Describes where to use the new completions
				"method": "replace", //replace or append
				"propose": [ //What to propose
					{
						"function": "$parse_documentation(entities, string, minecraft:)"
					}
				] 
			},
			{
				"recognize": "$is(minecraft:entity)",
				"method": "append",
				"propose": [
					{
						"key": "do_not_upgrade",
						"type": "object"
					},
					{
						"key": "component_groups",
						"type": "object"
					},
					{
						"key": "components",
						"type": "object"
					},
					{
						"key": "events",
						"type": "object"
					}
				]
			},
			{
				"recognize": "($contains(event) and $is_not(event) and $is_not(events))",
				"method": "replace",
				"propose": [
					{
						"key": "event",
						"type": "object"
					},
					{
						"key": "target",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_type(event)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_file(minecraft:entity/events, string)",
						"type": "string"
					}
				]
			},

			//COMPONENTS
			{
				"recognize": "($is_path(minecraft:entity/components) or $is_path(minecraft:entity/component_groups/.))",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(components, object, , true)", //The comma placement is not a bug; hands over empty string ""!
						"type": "object"
					}
				]
			},
			{
				"recognize": "($is_path(minecraft:entity/components/.) or $is_path(minecraft:entity/component_groups/./.))",
				"method": "replace",
				"propose": [
					{
						"function": "$get_component_args",
						"type": "object"
					}
				]
			},
			{
				"recognize": "($is_path(minecraft:entity/components/./.) or $is_path(minecraft:entity/component_groups/././.))",
				"method": "replace",
				"propose": [
					{
						"function": "$get_component_values",
						"type": "value"
					}
				]
			},

			//EVENTS
			{
				"recognize": "$is_path(minecraft:entity/events)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(events, object, , true)",  //The comma placement is not a bug; hands over empty string ""!
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/.) or $is_path(../randomize/.) or $is_path(../sequence/.)",
				"method": "replace",
				"propose": [
					{
						"key": "add",
						"type": "object"
					},
					{
						"key": "remove",
						"type": "object"
					},
					{
						"key": "sequence",
						"type": "list"
					},
					{
						"key": "randomize",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is_path(../randomize/.)",
				"method": "append",
				"propose": [
					{
						"key": "weight",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(randomize) or $is(sequence)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(add) or $is(remove) and $is_path(minecraft:entity/events/..)",
				"method": "replace",
				"propose": [
					{
						"key": "component_groups",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/..) and $is(component_groups)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/..) and $is_path(../component_groups/.)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_file(minecraft:entity/component_groups, string)",
						"type": "string"
					}
				]
			},

			//FILTERS
			{
				"recognize": "$contains(on_) or $is_path(../sequence/.)",
				"method": "replace",
				"propose": [
					{
						"key": "filters",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(filters)",
				"method": "replace",
				"propose": [
					{
						"key": "any_of",
						"type": "list"
					},
					{
						"key": "all_of",
						"type": "list"
					},
					{
						"function": "$parse_documentation(filter_basic, object, , true)",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(any_of) or $is(all_of)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../any_of/.) or $is_path(../all_of/.)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(filter_basic, object, , true)",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../any_of/./test) or $is_path(../all_of/./test) or $is_path(../filters/test)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(filter_names, string, , false)",
						"type": "string"
					}
				]
			}
		]
	},
	"loot_table": {
		"define": "$is_path(pools/..)",
		"basic": [ "vanilla.loot_tables" ],
		"extend": [
			{
				"recognize": "$is(pools)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../pools/.)",
				"method": "replace",
				"propose": [
					{
						"key": "rolls",
						"type": "object"
					},
					{
						"key": "tiers",
						"type": "object"
					},
					{
						"key": "entries",
						"type": "list"
					},
					{
						"key": "conditions",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is(tiers)",
				"method": "replace",
				"propose": [
					{
						"key": "initial_range",
						"type": "object"
					},
					{
						"key": "bonus_rolls",
						"type": "object"
					},
					{
						"key": "bonus_chance",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(entries)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../entries/.)",
				"method": "replace",
				"propose": [
					{
						"key": "type",
						"type": "object"
					},
					{
						"key": "name",
						"type": "object"
					},
					{
						"key": "weight",
						"type": "object"
					},
					{
						"key": "quality",
						"type": "object"
					},
					{
						"key": "functions",
						"type": "list"
					},
					{
						"key": "pools",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is_path(../entries/functions)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../entries/functions/.)",
				"method": "replace",
				"propose": [
					{
						"key": "function",
						"type": "object"
					},
					{
						"key": "chance",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../entries/./name) and $is(item)_on_$sibling_value(type)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(items, string, minecraft:)"
					},
					{
						"function": "$parse_documentation(blocks, string, minecraft:)"
					}
				]
			},
			{
				"recognize": "$is(type)",
				"method": "replace",
				"propose": [
					{
						"key": "loot_table",
						"type": "string"
					},
					{
						"key": "item",
						"type": "string"
					}
				]
			}
		]
	},
	"trade": {
		"define": "$is_path(tiers/..)",
		"basic": [ "vanilla.trades" ],
		"extend": [
			{
				"recognize": "$is(tiers)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../tiers/.)",
				"method": "replace",
				"propose": [
					{
						"key": "trades",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(trades)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../trades/.)",
				"method": "replace",
				"propose": [
					{
						"key": "wants",
						"type": "object"
					},
					{
						"key": "gives",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(wants) or $is(gives)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../wants/.) or $is_path(../gives/.)",
				"method": "replace",
				"propose": [
					{
						"key": "item",
						"type": "object"
					},
					{
						"key": "quantity",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(item)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(items, string, minecraft:)"
					},
					{
						"function": "$parse_documentation(blocks, string, minecraft:)"
					}
				] 
			},
			{
				"recognize": "$is(quantity)",
				"method": "replace",
				"propose": [
					{
						"key": "1",
						"type": "number"
					}
				] 
			}
		]
	},
	"recipe": {
		"define": "$contains(crafting)_on_$value",
		"basic": [ "vanilla.recipe" ],
		"extend": [
			{
				"recognize": "$is_root",
				"method": "replace",
				"propose": [
					{
						"key": "type",
						"type": "object"
					},
					{
						"key": "group",
						"type": "object"
					},
					{
						"key": "result",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_root and $is(crafting_shapeless)_on_$child_value(type)",
				"method": "replace",
				"propose": [
					{
						"key": "ingredients",
						"type": "object"
					}
				] 
			},
			{
				"recognize": "$is_root and $is(crafting_shaped)_on_$child_value(type)",
				"method": "replace",
				"propose": [
					{
						"key": "pattern",
						"type": "object"
					},
					{
						"key": "key",
						"type": "object"
					}
				] 
			},
			{
				"recognize": "$is(type)",
				"method": "replace",
				"propose": [
					{
						"key": "crafting_shaped",
						"type": "string"
					},
					{
						"key": "crafting_shapeless",
						"type": "string"
					}
				] 
			},
			{
				"recognize": "$is(pattern)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index(2)",
						"type": "object"
					}
				] 
			},
			{
				"recognize": "$is(key)",
				"method": "replace",
				"propose": [
					{
						"function": "$as_chars(object)_on_$parse_file(pattern, object, , false)",
						"type": "object"
					}
				] 
			},
			{
				"recognize": "$is(result) or $is_path(key/.)",
				"method": "replace",
				"propose": [
					{
						"key": "item",
						"type": "object"
					},
					{
						"key": "data",
						"type": "object"
					}
				] 
			},
			{
				"recognize": "$is(result)",
				"method": "append",
				"propose": [
					{
						"key": "count",
						"type": "object"
					}
				] 
			},
			{
				"recognize": "$is(item)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(items, string, minecraft:)"
					},
					{
						"function": "$parse_documentation(blocks, string, minecraft:)"
					}
				] 
			},
			{
				"recognize": "$is(data)",
				"method": "replace",
				"propose": [
					{
						"key": "0",
						"type": "number"
					}
				] 
			},
			{
				"recognize": "$is(count)",
				"method": "replace",
				"propose": [
					{
						"key": "1",
						"type": "number"
					}
				] 
			}
		]
	},
	"ui": {
		"define": "$has_child(type) or $is(namespace) or $contains(@)",
		"basic": [ "vanilla.ui" ],
		"extend": [
			{
				"recognize": "$is_path(.) or $is_path(../controls/.) and $contains_not(@)",
				"method": "replace",
				"propose": [
					{
						"key": "type",
						"type": "object"
					}
				] 
			},

			{
				"recognize": "$is(type)",
				"method": "replace",
				"propose": [
					{
						"key": "image",
						"type": "string"
					},
					{
						"key": "stack_panel",
						"type": "string"
					},
					{
						"key": "panel",
						"type": "string"
					},
					{
						"key": "label",
						"type": "string"
					}
				] 
			}
		]
	}
}