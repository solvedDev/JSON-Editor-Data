{
	"entity": {
		"define": "$is_path(minecraft:entity/..)", //Defines what an entity file is
		"basic": [ "vanilla.entities", "documentation.components" ], //Links to var
		"extend": [
			//GENERAL
			{
				"recognize": "($is(id) or $contains(entity_type) or $is_path(../transformation/into))", //Describes where to use the new completions
				"method": "replace", //replace or append
				"propose": [ //What to propose
					{
						"function": "$parse_documentation(entities, string, minecraft:)"
					}
				] 
			},
			{
				"recognize": "$is(minecraft:entity)",
				"method": "append",
				"propose": [
					{
						"key": "do_not_upgrade",
						"type": "object"
					},
					{
						"key": "component_groups",
						"type": "object"
					},
					{
						"key": "components",
						"type": "object"
					},
					{
						"key": "events",
						"type": "object"
					}
				]
			},
			{
				"recognize": "($contains(event) and $is_not(event) and $is_not(events))",
				"method": "replace",
				"propose": [
					{
						"key": "event",
						"type": "object"
					},
					{
						"key": "target",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(event)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_file(minecraft:entity/events, string)",
						"type": "string"
					}
				]
			},

			//COMPONENTS
			{
				"recognize": "($is(components) or $is_path(minecraft:entity/component_groups/.))",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(components, object, , true)", //The comma placement is not a bug; hands over empty string ""!
						"type": "object"
					}
				]
			},
			{
				"recognize": "($is_path(../components/.) or $is_path(minecraft:entity/component_groups/./.))",
				"method": "replace",
				"propose": [
					{
						"function": "$get_component_args",
						"type": "object"
					}
				]
			},
			{
				"recognize": "($is_path(../components/./.) or $is_path(minecraft:entity/component_groups/././.))",
				"method": "replace",
				"propose": [
					{
						"function": "$get_component_values",
						"type": "value"
					}
				]
			},

			//EVENTS
			{
				"recognize": "$is_path(minecraft:entity/events)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(events, object, , true)",  //The comma placement is not a bug; hands over empty string ""!
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/.) or $is_path(../randomize/.) or $is_path(../sequence/.)",
				"method": "replace",
				"propose": [
					{
						"key": "add",
						"type": "object"
					},
					{
						"key": "remove",
						"type": "object"
					},
					{
						"key": "sequence",
						"type": "list"
					},
					{
						"key": "randomize",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is_path(../randomize/.)",
				"method": "append",
				"propose": [
					{
						"key": "weight",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(randomize) or $is(sequence)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(add) or $is(remove) and $is_path(minecraft:entity/events/..)",
				"method": "replace",
				"propose": [
					{
						"key": "component_groups",
						"type": "list"
					},
					{
						"key": "effects",
						"type": "list"
					},
					{
						"key": "group",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/..) and $is(component_groups)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/..) and $is_path(../component_groups/.)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_file(minecraft:entity/component_groups, string)",
						"type": "string"
					}
				]
			},
			

			//FILTERS
			{
				"recognize": "$contains(on_) or $is_path(../sequence/.)",
				"method": "replace",
				"propose": [
					{
						"key": "filters",
						"type": "object"
					},
					{
						"key": "event",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is(filters)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(filter_basic, object, , true)",
						"type": "object"
					},
					{
						"key": "any_of",
						"type": "list"
					},
					{
						"key": "all_of",
						"type": "list"
					}
				]
			},
			{
				"recognize": "$is(any_of) or $is(all_of)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../any_of/.) or $is_path(../all_of/.)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(filter_basic, object, , true)",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../any_of/./test) or $is_path(../all_of/./test) or $is_path(../filters/test)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(filter_names, string, , false)",
						"type": "string"
					}
				]
			},
			{
				"recognize": "$is_path(../filters/..) and $is(subject)",
				"method": "replace",
				"propose": [
					{
						"key": "self",
						"type": "string"
					},
					{
						"key": "other",
						"type": "string"
					},
					{
						"key": "parent",
						"type": "string"
					},
					{
						"key": "player",
						"type": "string"
					},
					{
						"key": "target",
						"type": "string"
					}
				]
			},
			{
				"recognize": "$is_path(../filters/..) and $is(value) and $is(has_equipment)_on_$sibling_value(test)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(items, string)"
					},
					{
						"function": "$parse_documentation(blocks, string)"
					}
				]
			},

			//CUSTOM
			{
				"recognize": "$is_path(minecraft:entity/events/.)",
				"method": "append",
				"propose": [
					{
						"key": "spawn",
						"type": "object"
					},
					{
						"key": "redirect",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/./spawn)",
				"method": "replace",
				"propose": [
					{
						"key": "entity",
						"type": "object"
					},
					{
						"key": "item",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../events/./spawn/entity) or $is_path(../events/./spawn/item)",
				"method": "replace",
				"propose": [
					{
						"key": "value",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../events/./spawn/entity/value)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(entities, string, minecraft:)"
					}
				]
			},
			{
				"recognize": "$is_path(../events/./spawn/item/value)",
				"method": "replace",
				"propose": [
					{
						"function": "$parse_documentation(items, string)"
					},
					{
						"function": "$parse_documentation(blocks, string)"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/./redirect)",
				"method": "replace",
				"propose": [
					{
						"key": "filters",
						"type": "object"
					},
					{
						"key": "event",
						"type": "object"
					},
					{
						"key": "range",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(minecraft:entity/events/./redirect/range)",
				"method": "replace",
				"propose": [
					{
						"key": "range",
						"type": "16"
					}
				]
			},
			{
				"recognize": "$is(group) and $is_path(../events/..)",
				"method": "replace",
				"propose": [
					{
						"key": "name",
						"type": "object"
					},
					{
						"key": "components",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$contains(effects) and $is_not(spell_effects) and $is_not(remove_effects)",
				"method": "replace",
				"propose": [
					{
						"function": "$next_list_index",
						"type": "object"
					}
				]
			},
			{
				"recognize": "$is_path(../effects/.) or $is_path(../add_effects/.)",
				"method": "replace",
				"propose": [
					{
						"key": "effect",
						"type": "object"
					},
					{
						"key": "duration",
						"type": "object"
					},
					{
						"key": "amplifier",
						"type": "object"
					},
					{
						"key": "visible",
						"type": "object"
					},
					{
						"key": "ambient",
						"type": "object"
					}
				]
			}
		]
	},
